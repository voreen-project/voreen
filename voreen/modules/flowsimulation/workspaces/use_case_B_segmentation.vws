#include "modules/mod_sampler2d.frag"

uniform sampler2D colorTex_;
uniform sampler2D depthTex_;
uniform TextureParameters texParams_;

float lowerThresh = 0.25;
float upperThresh = 0.75;

//vec4 outlineColorFG = vec4(1.0, 0.15, 0.0, 1.0);
//vec4 outlineColorBG = vec4(0.0, 0.85, 0.9, 1.0);
vec4 outlineColorFG = vec4(0.9, 0.9, 0.0, 1.0);
vec4 outlineColorBG = vec4(0.9, 0.9, 0.0, 1.0);

vec4 clearColor = vec4(0.0, 0.0, 0.0, 0.0);

float contrast = 1.0;
float bias = 0.001;

void main() {
    vec2 p = gl_FragCoord.xy * screenDimRCP_;
    vec4 fragColor = textureLookup2Dnormalized(colorTex_, texParams_, p);
	vec4 leftCol = textureLookup2D(colorTex_, texParams_, gl_FragCoord.xy + vec2(-1.0,0.0));
	vec4 rightCol = textureLookup2D(colorTex_, texParams_, gl_FragCoord.xy + vec2(1.0,0.0));
	vec4 topCol = textureLookup2D(colorTex_, texParams_, gl_FragCoord.xy + vec2(0.0,1.0));
	vec4 bottomCol = textureLookup2D(colorTex_, texParams_, gl_FragCoord.xy + vec2(0.0,-1.0));
	
    bool contour = (fragColor != leftCol || fragColor != rightCol || 
        fragColor != topCol || fragColor != bottomCol);
    contour = true;
    if (fragColor != leftCol || fragColor != rightCol || 
        fragColor != topCol || fragColor != bottomCol || true) {
	    
        float minCol = min(leftCol.x, rightCol.x);
        minCol = min(minCol, topCol.x);
        minCol = min(minCol, bottomCol.x);
        if (fragColor.x > minCol || !contour) {
            float probability = fragColor.x;
            float blendFactor = clamp((upperThresh - probability - bias) / (upperThresh - lowerThresh), 0.0, 1.0);
            if (blendFactor < 0.50) {
                //blendFactor = pow(blendFactor*2.0, contrast) / 2.0;
                /*blendFactor *= 2.0;
                fragColor.xyz = mix(outlineColorBG, outlineColorFG, blendFactor).xyz; */
                fragColor.xyz = outlineColorFG.xyz;
                fragColor.a = clamp(1.0 - (0.5 - blendFactor)*1.5, 0.0, 1.0);
                //fragColor.a *= 0.5;
                /*if (!contour)
                    fragColor.a *= 0.5;*/
            }
            else {
                //blendFactor = 1.0 - (pow((1.0 - blendFactor) * 2.0, contrast) / 2.0);
                //blendFactor = 2.0*(blendFactor-0.5);
                //fragColor.xyz = mix(outlineColorFG, outlineColorBG, blendFactor).xyz;
                //fragColor.xyz = mix(outlineColorFG, outlineColorBG, blendFactor).xyz;
                fragColor.xyz = outlineColorBG.xyz;
                fragColor.a = clamp(1.0 - (blendFactor - 0.5)*1.5, 0.0, 1.0);
                //fragColor.a *= 0.9;

               /* if (!contour)
                    fragColor.a *= 0.25;*/
            }
            blendFactor = clamp(blendFactor, 0.0, 1.0);            

            //fragColor.xyz = mix(outlineColorFG, outlineColorBG, blendFactor).xyz;
            //fragColor.a = 1.0;
            //fragColor.a = 1.0 - abs(probability - 0.5)/(upperThresh-lowerThresh);

        }
        else {
			fragColor = vec4(0.0);
        }
	}
    else {
        fragColor = vec4(0.0);
	}

    //fragColor.a = 0.1;
    gl_FragColor = fragColor;
    gl_FragDepth = textureLookup2Dnormalized(depthTex_, texParams_, p).z;
}
