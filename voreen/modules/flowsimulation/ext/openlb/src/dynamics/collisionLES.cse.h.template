% for name, DESCRIPTOR in descriptors.items():
template <typename... FIELDS>
struct KrauseEffectiveOmega<PerPopulationBGK,descriptors::${name}<FIELDS...>,momenta::BulkTuple,equilibria::SecondOrder> {

